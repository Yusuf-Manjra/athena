package TrigCosmicEvent

author Takanori Kono <Takanori.Kohno@cern.ch>

use AtlasPolicy AtlasPolicy-*
use GaudiInterface GaudiInterface-* External

use DataModel   DataModel-*   Control
use CLIDSvc     CLIDSvc-*     Control
use AtlasCLHEP  AtlasCLHEP-*  External
use TrigInDetEvent TrigInDetEvent-* Trigger/TrigEvent
use SGTools     SGTools-*     Control

use TrigNavigation TrigNavigation-* Trigger/TrigEvent

library TrigCosmicEvent *.cxx -s=components *.cxx

apply_pattern installed_library

private
use AtlasReflex AtlasReflex-* External -no_auto_imports
use TrigSerializeUtils      TrigSerializeUtils-*    Trigger/TrigDataAccess 
use TrigSerializeCnvSvc     TrigSerializeCnvSvc-*   Trigger/TrigDataAccess

apply_pattern lcgdict dict=TrigCosmicEvent selectionfile=selection.xml \
	headerfiles="../TrigCosmicEvent/TrigCosmicEventDict.h"

apply_pattern sercnv files="-s=${TrigCosmicEvent_root}/TrigCosmicEvent CosmicMuonCollection.h MdtTrackSegmentCollection.h"

end_private
