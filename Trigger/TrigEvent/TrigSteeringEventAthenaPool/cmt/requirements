macro TrigSteeringEventAthenaPool_whattocompile "TrigRoiDescriptor.h TrigRoiDescriptorCollection.h TrigPassBits.h TrigPassBitsCollection.h TrigPassFlags.h TrigPassFlagsCollection.h GenericResult.h Lvl1Result.h HLTResult.h TrigOperationalInfo.h TrigOperationalInfoCollection.h" AthAnalysisBase " TrigPassBits.h TrigPassBitsCollection.h TrigPassFlags.h TrigPassFlagsCollection.h "


apply_pattern poolcnv typesWithNamespace="HLT::HLTResult LVL1CTP::Lvl1Result" \
      files="-s=${TrigSteeringEvent_root}/TrigSteeringEvent $(TrigSteeringEventAthenaPool_whattocompile)"

apply_pattern declare_joboptions files="*.py"

