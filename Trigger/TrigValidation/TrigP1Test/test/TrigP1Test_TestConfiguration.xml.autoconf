    <rtt  xmlns="http://www.hep.ucl.ac.uk/atlas/AtlasTesting/rtt">
        
        <rttContactPerson>David Strom</rttContactPerson>
        <mailto>David.Strom@cern.ch</mailto>
        <refRelease>15.5.X.Y</refRelease>
        
        <jobList>
            
            <classification>
                <displayClass>OnlineValidation</displayClass>
                <displayProcess>Core</displayProcess>
                <displayComponent>Trig-Common</displayComponent>
            </classification>    
            
            <!-- ENABLE-IN-PROJECT-BEGIN: AtlasHLT AtlasP1HLT AtlasCAFHLT AtlasTestHLT -->
            
            <jobTransform userJobId="All_data_run_stop_run_chain" >
                <doc>
                    athenaHLT on DATA all slices Stop_Run_Stop
                </doc>
                <jobTransformJobName>All_data_run_stop_run_chain</jobTransformJobName>
                <jobTransformCmd>
                    get_files run-stop-run-saveHist-rtt.trans; athenaHLT.py --perfmon -i -M -F /afs/cern.ch/atlas/project/trigger/pesa-sw/validation/RTT/data/BS/rttEBdataRunStopRunEvents.py -o All_data_run_stop_run -c "testPhysicsV5=True;rerunLVL1=True;doValidation=True" TrigP1Test/runHLT_standaloneRTT.py &lt; run-stop-run-saveHist-rtt.trans
                </jobTransformCmd>
                <group>AtlasHLTChainTest</group>
                <queue>long</queue>
            </jobTransform>
            
            <jobTransform userJobId="athenaHLT_on_data_leakCheck" >
                <doc>
                    athenaHLT on DATA all slices leakCheck
                </doc>
                <jobTransformJobName>athenaHLT_on_data_leakCheck</jobTransformJobName>
