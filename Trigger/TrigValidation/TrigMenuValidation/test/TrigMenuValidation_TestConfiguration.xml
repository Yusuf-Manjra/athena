<?xml version="1.0"?>
<!DOCTYPE unifiedTestConfiguration SYSTEM "http://www.hep.ucl.ac.uk/atlas/AtlasTesting/DTD/unifiedTestConfiguration.dtd">
<unifiedTestConfiguration>

  <atn/>
  <kv/>

   <rtt xmlns="http://www.hep.ucl.ac.uk/atlas/AtlasTesting/rtt">
      <mailto>Iwona.Grabowska@cern.ch</mailto>
      <refRelease>14.5.0</refRelease>

      <jobList>
      <classification>
         <displayClass>OfflineValidation</displayClass>
         <displayProcess>Reco</displayProcess>
         <displayComponent>Trig-Other</displayComponent>
      </classification>

         <athena userJobId='TrigMenuValidation_XMLUploadToDB'>
            <doc>Uploads a set of trigger menus to the data base and test with 10**31 menu with prescales and enhanced bias A</doc>
            <displayName>DBSimpleUploadFromInstallArea</displayName>
	    <commandLineFlags>DetDescrVersion="ATLAS-GEO-16-00-00";EvtMax=1000;OutputLevel=WARNING;LVL1OutputLevel=WARNING;HLTOutputLevel=WARNING</commandLineFlags>
            <options>TrigMenuValidation_RTT_XMLUploadToDB</options>
            <package>Trigger/TrigValidation/TrigMenuValidation</package>
            <group>TrigMenuValidation</group>
            <queue>long</queue>
	    <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000001.pool.root.1</dataset>
            <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000002.pool.root.1</dataset>
            <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000003.pool.root.1</dataset>
      
         <athenaCommonFlags />
         </athena>

         <athena userJobId='TrigMenuValidation_XMLUploadToDBwithSettings'>
            <doc>Uploads a set of trigger menus along with algorithms settings to a SQLite file</doc>
            <displayName>DBUploadWithAlgorithmProperties</displayName>
	    <commandLineFlags>DetDescrVersion="ATLAS-GEO-16-00-00";EvtMax=1000;OutputLevel=WARNING;LVL1OutputLevel=WARNING;HLTOutputLevel=WARNING</commandLineFlags>
            <options>TrigMenuValidation_RTT_XMLUploadToDBwithSettings</options>
            <package>Trigger/TrigValidation/TrigMenuValidation</package>
            <group>TrigMenuValidation</group>
            <queue>long</queue>
	    <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000001.pool.root.1</dataset>
            <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000002.pool.root.1</dataset>
            <dataset>/castor/cern.ch/atlas/atlascerngroupdisk/proj-sit/igrabows/TrigInDetValidation_bjet5200/RDO.213802._000003.pool.root.1</dataset>
      
         <athenaCommonFlags />
         </athena>

      </jobList>



      <jobGroups>
         <jobGroup name="AthenaTrigMenuValidation" parent="Athena">
            <keepFilePattern displayColor="blue">TriggerMenuSQLiteFile*.sqlite</keepFilePattern>
            <keepFilePattern displayColor="blue">MenusKeys*txt</keepFilePattern>
            <keepFilePattern displayColor="blue">expert-monitoring*root</keepFilePattern>
            <keepFilePattern displayColor="green">CompMenu-*txt</keepFilePattern>
            <keepFilePattern displayColor="green">HLTconfig_*xml</keepFilePattern>
            <keepFilePattern displayColor="green">HLTsetup_*xml</keepFilePattern>
            <keepFilePattern displayColor="green">LVL1config*xml</keepFilePattern>
            <keepFilePattern displayColor="green">MC_*log</keepFilePattern>

   	    <auxFilePattern>XMLUploadToDB.sh</auxFilePattern>
   	    <auxFilePattern>XMLDumperFromAthenaWithSettings.sh</auxFilePattern>

       </jobGroup>
      </jobGroups>
   </rtt>

</unifiedTestConfiguration>
