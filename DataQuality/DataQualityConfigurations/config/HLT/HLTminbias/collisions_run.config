#######################
# HLTminbias
#######################

#######################
# References
#######################

reference HLTminbias_Ref {
  file = /afs/cern.ch/user/a/atlasdqm/dqmdisk/tier0/han_references/Collisions/data12_8TeV.00200805.physics_MinBias.merge.HIST.f432_m1108._0001.1_sedwardg_120409.root
  path = run_200805
  name = same_name
}

#######################
# Output
#######################

output top_level {
  output HLT {
    output TRMBI {
      output SHIFTER {
      }
      output EXPERT {
        output General {
        }
        output MBTS {
        }
        output HMT {
	  output mb_sp1400_hmtperf_L1TE10 {
	  }
	  output mb_sp1400_hmtperf_L1TE10.0ETA25 {
	  }
	  output mb_sp1400_hmtperf_L1TE20 {
	  }
	  output mb_sp1400_hmtperf_L1TE20.0ETA25 {
	  }
        }
        output IDMinbias {
        }
        output ALFA {
	  output mb_sptrk_vetombts2in_L1ALFA_CEP {
	  }
	  output mb_sptrk_vetombts2in_peb_L1ALFA_ANY {
	  }
	}
      }
    }
  }
}

#######################
# Histogram Assessments
#######################

dir HLT {
  algorithm = HLTminbias_GatherData
  dir MinBiasMon {
    #histos for shifter dir
    hist TriggerEntries@Shifter {
      output = HLT/TRMBI/SHIFTER
    }
    hist TriggerEfficiencies@Shifter {
      output = HLT/TRMBI/SHIFTER
    }
    
    dir MBTS {
      dir mb_perf_L1MBTS_2 {
        hist Occupancy@Shifter {
          output = HLT/TRMBI/SHIFTER
        }
        hist MbtsCorr_N_N@Shifter {
	  output = HLT/TRMBI/SHIFTER
	  display = LogZ
      	}
	hist Time@Shifter {
          output = HLT/TRMBI/SHIFTER
        }
      }
    }

    dir IDMinbias {
      dir mb_sptrk {
        hist PixTot@Shifter {
          output = HLT/TRMBI/SHIFTER
        }
        hist SctTot@Shifter {
          output = HLT/TRMBI/SHIFTER 
        }
      }
    }

    #histos for general   
    hist TriggerPurities@General {
      output = HLT/TRMBI/EXPERT/General
    } 
    hist TriggerEntries@General {
      output = HLT/TRMBI/EXPERT/General
    }
    
    hist TriggerEfficiencies@General {
      output = HLT/TRMBI/EXPERT/General
    }

    dir Purities&Efficiencies {
      dir mb_sptrk {
	output = HLT/TRMBI/EXPERT/General
	hist NumGoodOfflineTracks@General {
	  display = LogY
	}
	hist GoodOfflineTracksPt@General {
	  display = LogY
	}
	hist Purity@General {
	}
      }
    } 
    
    
    #now for HLT/TRMBI/EXPERT/MBTS dir
    dir MBTS {
      dir mb_perf_L1MBTS_2 {
	output = HLT/TRMBI/EXPERT/MBTS
        hist Occupancy@MBTS {
        }
        hist OccupancyOnline@MBTS { 
        }
        hist Time@MBTS {
        }
        hist TimeOnline@MBTS {
        }
        hist MbtsEnergyErrors@MBTS {
        }
        hist MbtsTimeErrors@MBTS {
        }     
        hist MbtsCorr_N_N@MBTS {
	  display = LogZ
        }
      }
    }
    
    # now for IDMinbias
    dir IDMinbias {
      dir mb_sptrk {
	output = HLT/TRMBI/EXPERT/IDMinbias
	hist PixBarr_SP@IDMinbias {
	}
	hist PixECA_SP@IDMinbias {
	}
	hist PixECC_SP@IDMinbias {
	}
	hist SctBarr_SP@IDMinbias {
	}
	hist SctECA_SP@IDMinbias {
	}
	hist SctECC_SP@IDMinbias {
	}
	hist PixTot@IDMinbias {
	}
	hist SctTot@IDMinbias {
	}
	hist MinbiasTracks@IDMinbias {
	}
      }
    }

    # now for HMT   
    dir HMT {
      dir mb_sp1400_hmtperf_L1TE10 {
	output = HLT/TRMBI/EXPERT/HMT/mb_sp1400_hmtperf_L1TE10
        hist NumSpacePoints@HMT1 {
        }
        hist NumVertices@HMT1 {
        }
        hist NumTracksAtVertex@HMT1 {
        }
        hist NumHitsAtVertex@HMT1 {
        }
      }
      dir mb_sp1400_hmtperf_L1TE10.0ETA25 {
	output = HLT/TRMBI/EXPERT/HMT/mb_sp1400_hmtperf_L1TE10.0ETA25
        hist NumSpacePoints@HMT2 {
	}
	hist NumVertices@HMT2 {
	}
	hist NumTracksAtVertex@HMT2 {
	}
	hist NumHitsAtVertex@HMT2 {
	}
      }
      dir mb_sp1400_hmtperf_L1TE20 {
	output = HLT/TRMBI/EXPERT/HMT/mb_sp1400_hmtperf_L1TE20
	hist NumSpacePoints@HMT3 {
	}
	hist NumVertices@HMT3 {
	}
	hist NumTracksAtVertex@HMT3 {
	}
	hist NumHitsAtVertex@HMT3 {
	}
      }
      dir mb_sp1400_hmtperf_L1TE20.0ETA25 {
	output = HLT/TRMBI/EXPERT/HMT/mb_sp1400_hmtperf_L1TE20.0ETA25
	hist NumSpacePoints@HMT4 {
	}
	hist NumVertices@HMT4 {
	}
	hist NumTracksAtVertex@HMT4 {
	}
	hist NumHitsAtVertex@HMT4 {
	}
      }
    }
    #now for ALFA
    dir Purities&Efficiencies {
      dir mb_sptrk_vetombts2in_peb_L1ALFA_ANY {
	 output = HLT/TRMBI/EXPERT/ALFA/mb_sptrk_vetombts2in_peb_L1ALFA_ANY
	 hist Purity@ALFA1 {
	 }
	 hist NumGoodOfflineTracks@ALFA1 {
	   display = LogY
	 }
	 hist GoodOfflineTracksPt@ALFA1 {
	   display = LogY
	 }
      }
      dir mb_sptrk_vetombts2in_L1ALFA_CEP {
	output = HLT/TRMBI/EXPERT/ALFA/mb_sptrk_vetombts2in_L1ALFA_CEP
	hist Purity@ALFA2 {
	}
	hist NumGoodOfflineTracks@ALFA2 {
	  display = LogY
	}
	hist GoodOfflineTracksPt@ALFA2 {
	  display = LogY
	}
      }
    }
    dir MBTS {
      dir mb_sptrk_vetombts2in_peb_L1ALFA_ANY {
	output = HLT/TRMBI/EXPERT/ALFA/mb_sptrk_vetombts2in_peb_L1ALFA_ANY
	hist OccupancyOnline@ALFA1 { 
	}
	hist TimeOnline@ALFA1 {
	}    
	hist MbtsCorr_N_N@ALFA1 {
	  display = LogZ
	}
      }
      dir mb_sptrk_vetombts2in_L1ALFA_CEP {
	output = HLT/TRMBI/EXPERT/ALFA/mb_sptrk_vetombts2in_L1ALFA_CEP
	hist OccupancyOnline@ALFA2 { 
	}
	hist TimeOnline@ALFA2 {
	}    
	hist MbtsCorr_N_N@ALFA2 {
	  display = LogZ
	}
      }
    }
  }
}

##############
# Algorithms
##############

algorithm HLTminbias_Histogram_Not_Empty&GatherData {
  libname = libdqm_algorithms.so
  name = HLT_Histogram_Not_Empty&GatherData
  reference = HLTminbias_Ref
} 

algorithm HLTminbias_GatherData {
  libname = libdqm_algorithms.so
  name = GatherData
}


