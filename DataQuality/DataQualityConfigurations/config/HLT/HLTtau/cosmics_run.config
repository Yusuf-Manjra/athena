#######################
# HLTtau
#######################

#######################
# Output
#######################

output top_level {
	algorithm = WorstCaseSummary
  	output HLT {
    		output TRTAU {      
			output Expert {
                                output tau8_cosmic_track {
                                        output L1RoI {
                                        }
                                        output PreselectionTau {
                                        }
                                        output EFTau {
                                        }
                                }

                                output tau8_cosmic_ptonly {
                                        output L1RoI {
                                        }
                                        output PreselectionTau {
                                        }
                                        output EFTau {  
                                        }
                                }
			}
            		output Shifter {
				output cosmic_chain {
                                        output L1RoI {
                                        }
                                        output EFTau {
                                        }
                                }
			}
  		}
	}
}

#######################
# Histogram Assessments
#######################

# the tauNoCut_cosmic sections will produce warnings but override the description for
# some selected histograms

dir HLT {
#  algorithm = HLT_TAU_Histogram_Not_Empty&GatherData
#  output = HLT/TRTAU  
	dir TauMon {
		dir Expert {
			dir tau8_cosmic_track {
                                        dir L1RoI {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_track/L1RoI
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }
					}
                                        dir PreselectionTau {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_track/PreselectionTau
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }
                                        }
                                        dir EFTau {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_track/EFTau
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }                        
					}
			}
			dir tau8_cosmic_ptonly {
                                        dir L1RoI {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_ptonly/L1RoI
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }
                                        }
                                        dir PreselectionTau {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_ptonly/PreselectionTau
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }
                                        }
                                        dir EFTau {
                                                output = HLT/TRTAU/Expert/tau8_cosmic_ptonly/EFTau
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
                                                        regex = 1
                                                }
                                        }
                        }
			dir Shifter {
	    			dir cosmic_chain {
					dir L1RoI {
						output = HLT/TRTAU/Shifter/cosmic_chain/L1RoI
		    				hist h.* {
			   				algorithm = Histogram_Not_Empty
			   				display = StatBox
							regex = 1
						}
					}
                                        dir EFTau {
                                                output = HLT/TRTAU/Shifter/cosmic_chain/EFTau
                                                hist h.* {
                                                        algorithm = Histogram_Not_Empty
                                                        display = StatBox
							regex = 1
                                                }
                                        }
				}
			}
		}
	}
}
