    }
    hist EnergyAccetaphiLAr@ShifterL2 {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData 
        output = HLT/OfflineShifter/Eta-Phi-Maps/Calo
    }
  }


  dir ResultMon {
    dir AllChains {
      hist [^L].*RoI.*@Shifter {
        regex = 1
        algorithm = HLT_Histogram_Not_Empty&GatherData
        output = HLT/OfflineShifter/Eta-Phi-Maps
      }
    }
    
    dir Electrons {
      hist [^L].*RoI.*@Shifter {
        regex = 1
        algorithm = HLT_Histogram_Not_Empty&GatherData
        output = HLT/OfflineShifter/Eta-Phi-Maps
      }
    }
    
    dir Gamma {
      hist [^L].*RoI.*@Shifter {
        regex = 1
        algorithm = HLT_Histogram_Not_Empty&GatherData
        output = HLT/OfflineShifter/Eta-Phi-Maps
      }
    }
    
    dir Muons {
      hist [^L].*RoI.*@Shifter {
        regex = 1
        algorithm = HLT_Histogram_Not_Empty&GatherData
        output = HLT/OfflineShifter/Eta-Phi-Maps
      }
    }
    
    dir Taus {
      hist [^L].*RoI.*@Shifter {
        regex = 1
        algorithm = HLT_Histogram_Not_Empty&GatherData
        output = HLT/OfflineShifter/Eta-Phi-Maps
      }
    }
       
  }

