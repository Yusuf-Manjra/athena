#######################TRIGGER EFF######################################

dir HLT {
  dir JetMon {
    dir OF {
      dir AntiKt10LCTopoJets {
      	dir TrigEff {
          hist .*Eff_vs_pt {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty
            output = HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Turn-on
          }
          hist .*pt_vs_OF_pt {
            regex = 1  
            algorithm = HLTjet_Histogram_Not_Empty
            output = HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Correlation
          }
          hist .*Resolution_pt {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty
            output = HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Resolution
          }
          hist .*unmatched_pt {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty
            output = HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Unmatched_Jets
          }
        } ##End TrigEff
      } ## End AntiKt10LCTopoJets
    } ## End OF
  } ##End JetMon
} ##End HLT

