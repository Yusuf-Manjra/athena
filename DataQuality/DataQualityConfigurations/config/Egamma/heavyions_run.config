####################
# Thresholds
####################
thresholds egKolmogorovThr {
    limits MaxDist {
        warning = 0.05
            error = 0.15
    }
}

thresholds egamma_BinsDiffFromStripMedian_threshold {
  limits MaxDeviation {
    warning = 25
    error = 200
  }
}

####################
# Common Algorithms
####################

algorithm egamma_BinsDiffFromStripMedian {
  libname = libdqm_algorithms.so
  SuppressFactor = 0
  name = BinsDiffFromStripMedian
  thresholds = egamma_BinsDiffFromStripMedian_threshold
}

algorithm egKolmogorov {
    libname = libdqm_algorithms.so
        name = KolmogorovTest_MaxDist
        reference = CentrallyManagedReferences
        thresholds = egKolmogorovThr
        MinStat = 20
}

algorithm egamma_Histogram_Not_Empty {
        libname = libdqm_algorithms.so
        name = Histogram_Not_Empty
        reference = CentrallyManagedReferences
}

#############
# Output
#############

output top_level {
  output egamma {
   output photons { 
    output ID {
    }
    output Track { 
    }
    output EXPERT { 
    }
    output EXPERT_ID { 
    }
    output EXPERT_Track { 
    }
   }
   output electrons {
    output ID {
    }
    output Track {
    }
    output EXPERT {
    }
    output EXPERT_ID {
    }
    output EXPERT_Track {
    }
   }
   output forwardElectrons {
    output ID {
    }
    output EXPERT {
    }
  }
 }
}

