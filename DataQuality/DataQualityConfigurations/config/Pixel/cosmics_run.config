algorithm Pix_Bins_Diff_FromAvg {
  libname = libdqm_algorithms.so
  name = Bins_Diff_FromAvg
  thresholds = Pix_Diff_FromAvg
}
algorithm Pix_LB_Bins_Diff_FromAvg {
  libname = libdqm_algorithms.so
  name = Bins_Diff_FromAvg
  NSigma = 10
  PublishBins = 1
  thresholds = Pix_LB_Diff_FromAvg
}
