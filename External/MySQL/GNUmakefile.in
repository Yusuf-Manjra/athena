TARGET_REMAKE_CONFIGURE = yes
TARGET_REMAKE_MAKEFILES = GNUmakefile
TARGET_DATA = MySQL_env.csh:scripts/MySQL_env.csh\
              MySQL_env.sh:scripts/MySQL_env.sh
 
include $(SRT_HOME)/standard.mk
clean:; -rm -f MySQL* 
distclean-local:; -rm -f GNUmakefile




MySQL_env.sh: $(top_srcdir)/src/scripts/MySQL_env.sh.in
	cat $(top_srcdir)/src/scripts/MySQL_env.sh.in | sed 's%XXXMySQL_VERSIONXXX%$(MySQL_VERSION)%g' > MySQL_env.sh
MySQL_env.csh: $(top_srcdir)/src/scripts/MySQL_env.csh.in
	cat $(top_srcdir)/src/scripts/MySQL_env.csh.in | sed 's%XXXMySQL_VERSIONXXX%$(MySQL_VERSION)%g' > MySQL_env.csh

