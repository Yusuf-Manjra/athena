package AtlasMagField

author ATLAS Software Librarian <ATLAS-Software.Librarian@cern.ch>

use ExternalPolicy ExternalPolicy-00-* External

macro AtlasMagField_native_version current

macro ATLASMAGFIELD  "${SITEROOT}/atlas/project/magfield/CTB" \
      BNL            "${SITEROOT}/project/magfield/CTB" \
      LBN            "${SITEROOT}/local/project/magfield/CTB"

macro AtlasMagField_export_paths "${ATLASMAGFIELD}"
macro AtlasMagField_platform "" PACK noarch

path_remove DATAPATH "/project/magfield/CTB"
path_append DATAPATH "${ATLASMAGFIELD}"
