package MMMySQL

author Julius Hrivnac <Julius.Hrivnac@cern.ch>

branches doc

use JavaSDK JavaSDK-01-* External 
use AtlasExternalArea AtlasExternalArea-00-* External 
use Ant Ant-00-* External

macro MMMYSQL_VERSION "5.0.5"
macro MMMYSQL_HOME    "${JAVA_BASE}/mysql-connector/${MMMYSQL_VERSION}/share"

macro MMMySQL.core "${MMMYSQL_HOME}/mysql-connector-java-${MMMYSQL_VERSION}-bin.jar"

document installer install_jars "${MMMySQL.core}" install_dir="$(CMTINSTALLAREA)/share/lib"
apply_pattern install_docs 

macro MMMySQL_export_paths   "${MMMYSQL_HOME}"
macro MMMySQL_native_version "${MMMYSQL_VERSION}"
macro MMMySQL_platform "" PACK noarch




