 
${NAME}.install_pcms :  ${installDir}${NAME}Dict_rdict.pcm
	@echo ${NAME}Dict_rdict.pcm installed


${installDir}${NAME}Dict_rdict.pcm : ${bin}dict/${NAME}Dict_rdict.pcm
	@echo "Installing PCM  ${NAME}Dict_rdict.pcm" ; 
#	$(symlink_command) $< $@   # MN: need to get the correct dst path good for symlink ($bin is relative)
	$(atlas_install_command)  $<  $@ 



clean :: ${CONSTITUENT}clean
	@echo "${CONSTITUENT}clean  done"

${CONSTITUENT}clean::
	-rm -f ${installDir}${NAME}Dict_rdict.pcm
