
  <!-- Add in ElementLinkVector for ${class} -->
  <class name="ElementLinkVector< ${class} >" >
  </class>
  <exclusion>
    <class name="ElementLinkVector< ${class} >" >
      <!-- The signatures for these are often so long as to crash cint. -->
      <method name="insert"></method>
      <method name="erase"></method>
    </class>
  </exclusion>
  <class name="std::vector<SG::ELVRef< ${class} > >" >
  </class>
  <class name="SG::ELVRef< ${class} >">
    <field name="m_link" transient="true" > </field>
  </class>
  <class name="std::vector<DataLink< ${class} > >" >
  </class>
