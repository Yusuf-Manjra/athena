
${CONSTITUENT}clean ::
	@if test -d ${dict_dir} ; then \
	    file_list=`ls ${dict_dir}` ; \
	    for file_name in $${file_list} ; do  \
	        if test -f ${dict_dir}/$${file_name} ; then \
	            echo "Remove ${dict_dir}/$${file_name}" ; \
		    /bin/rm -f ${dict_dir}/$${file_name} ; \
		fi  \
	    done ; \
	fi 

