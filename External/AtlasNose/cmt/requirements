package AtlasNose
author  Sebastien Binet <binet@cern.ch>

use AtlasPolicy 	AtlasPolicy-*
use AtlasPython 	AtlasPython-*		External

private
macro nose_version "0.11.1"
apply_pattern distutils_build \
 name=nose \
 src_dir=$(AtlasNose_root)/src/nose-$(nose_version).tar.gz \
 install_dir=python
end_private
