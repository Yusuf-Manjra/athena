
${CONSTITUENT} :: $(bin)${CONSTITUENT}.stamp
	@echo ${CONSTITUENT} ok

$(bin)${CONSTITUENT}.stamp :: $(use_stamps)
	@cd $(bin); \
	  if test -f lib${library}.a; then touch lib${library}.a; fi; \
	  echo " Need to rebuild ${library} shared library"
	  touch ${CONSTITUENT}.stamp

${CONSTITUENT}clean ::
	@cd $(bin); /bin/rm -f ${library}.stamp
