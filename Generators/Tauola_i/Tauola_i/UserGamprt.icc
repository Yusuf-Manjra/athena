// Inline implementations for UserGamprt

// initialise pointer
void UserGamprt::init(void) {
  if (!_usergamprt){
    _usergamprt = static_cast<USERGAMPRT*>(usergamprt_address_());
    for(int i=0 ;i<nmax();i++){
      _usergamprt->usergamprt[i]=0.;
    }
    _usergamprt->ugused=0;

  }		
}	
